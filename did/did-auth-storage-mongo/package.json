  ],
  "devDependencies": {
    "j2d": "^1.0.3-dev",
    "jest": "^23.5.0",
    "remark-cli": "^5.0.0",
    "remark-preset-github": "^0.0.9"
  },
  "remarkConfig": {
    "plugins": [
      "preset-github",
      [
        "validate-links",
        {
          "repository": "ArcBlock/forge-js"
        }
      ]
