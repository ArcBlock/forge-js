{
  "env": {
    "development": {
      "presets": [
        [
          "babel-preset-env",
          {
            "targets": {
              "node": "8"
            }
          }
        ]
      ],
      "plugins": [["babel-plugin-inline-import", { "extensions": [".txt"] }]]
    },
    "browser": {
      "presets": [
        [
          "babel-preset-env",
          {
            "targets": {
              "browsers": "last 2 versions, ie 10-11"
            },
            "modules": false
          }
        ]
      ],
      "plugins": [["babel-plugin-inline-import", { "extensions": [".txt"] }]]
    }
  },
  "sourceMaps": true
}
